{
    "name": "react-clean-starter",
    "description": "Clear, Robust and Maintenable React Starter Kit with Clean Architecture and DDD",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test",
        "eject": "craco eject",
        "eslint:check": "eslint .",
        "eslint:fix": "eslint --fix .",
        "translations:gen": "npx ts-node bin/translation-keys-generator.ts",
        "translations:gen:watch": "ts-node-dev --respawn --watch ./public/assets/translations bin/translation-keys-generator.ts",
        "tailwind:process": "npx tailwindcss -i ./src/tailwind.css -o ./public/assets/css/app.css -c ./tailwind.config.js",
        "tailwind:watch": "npx tailwindcss -i ./src/tailwind.css -o ./public/assets/css/app.css -c ./tailwind.config.js --watch"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.14",
        "@heroicons/react": "^2.0.18",
        "@material-tailwind/react": "^2.0.1",
        "@remix-run/router": "^1.6.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.26",
        "@types/react": "18.2.5",
        "@types/react-dom": "18.2.4",
        "@uiw/react-head": "^1.0.4",
        "axios": "^1.4.0",
        "class-transformer": "^0.5.1",
        "detect-browser": "^5.3.0",
        "es6-shim": "^0.35.8",
        "i18next": "^22.4.15",
        "i18next-browser-languagedetector": "^7.0.1",
        "i18next-http-backend": "^2.2.0",
        "js-cookie": "^3.0.5",
        "moment": "^2.30.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.44.1",
        "react-i18next": "^12.2.2",
        "react-router-dom": "^6.11.1",
        "react-scripts": "5.0.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "socket.io-client": "^4.6.1",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@craco/craco": "^7.1.0",
        "@eslint/js": "^9.11.0",
        "@types/es6-shim": "^0.31.42",
        "@types/i18next-browser-languagedetector": "^3.0.0",
        "@types/inversify": "^2.0.33",
        "@types/js-cookie": "^3.0.3",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^8.6.0",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "eslint": "^9.10.0",
        "eslint-config-love": "^64.0.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-config-standard-with-typescript": "^32.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.35.2",
        "globals": "^15.9.0",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.3",
        "prettier-eslint": "^16.3.0",
        "tailwindcss": "^3.2.4",
        "ts-node-dev": "^2.0.0",
        "typescript-eslint": "^8.5.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}